{
  "name": "exchange",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "frontend",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev frontend",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "gen:lng-keys": "cd packages/languages && yarn gen:keys "
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.0.2",
    "eslint-config-custom": "*",
    "prettier": "^2.8.4",
    "prettier-plugin-organize-imports": "^3.2.2",
    "ts-node": "^10.9.1",
    "turbo": "^1.8.1",
    "prettier-plugin-tailwindcss": "^0.2.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@1.22.19"
}
