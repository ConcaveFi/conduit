{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "frontend": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalEnv": ["VERCEL_URL", "SOCKET_PLUGIN_KEY", "INCH_REFERRAL_ADDRESS"]
}
